{
  "name": "bytic/orm",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/ByTIC/orm",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Records\\": "src",
      "ByTIC\\ORM\\": "orm"
    },
    "classmap": [
      "src",
      "legacy"
    ],
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\ORM\\Tests\\": "tests/orm",
      "Nip\\Records\\Tests\\": "tests/src",
      "Nip\\Records\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "bytic/cache": "^1.0|^2.0",
    "bytic/collections": "^1.0|^2.0",
    "bytic/database": "^1.0|^2.0",
    "bytic/data-objects": "^1.0|^2.0",
    "bytic/helpers": "^1.0|^2.0",
    "bytic/inflector": "^1.0|^2.0",
    "bytic/namefy": "^1.0|^2.0",
    "bytic/utility": "^1.0|^2.0",
    "bytic/pipeline": "^1.0|^2.0"
  },
  "require-dev": {
    "bytic/container": "^1.0|^2.0",
    "bytic/orm-filters": "^1.0|^2.0",
    "bytic/http": "^1.0|^2.0",
    "bytic/event-dispatcher": "^1.0|^2.0",
    "bytic/phpqatools": "^1.0",
    "mockery/mockery": "^1.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
